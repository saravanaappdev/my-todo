{
  "expo": {
    "name": "Taskly",
    "slug": "taskly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4158D0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saravanaappdev.taskly"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4158D0"
      },
      "package": "com.saravanaappdev.taskly",
      "versionCode": 1
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "taskly",
    "extra": {
      "eas": {
        "projectId": "76cca6d9-df40-4202-81cf-7867583b652f"
      }
    },
    "owner": "saravanaappdev"
  }
}
